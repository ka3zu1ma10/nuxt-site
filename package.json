{
  "name": "nuxt_blog_template",
  "version": "1.0.0",
  "description": "nuxt_blog_template",
  "author": "urawa72",
  "private": true,
  "scripts": {
    "dev": "eslint --fix --ext .js,.vue --ignore-path .gitignore . && nuxt ",
    "build": "nuxt build",
    "start": "nuxt start",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "md": "processmd contents/blogs/md/**/*.md --stdout --outputDir contents/blogs/json > contents/blogs/summary.json",
    "jpg_copy": "node modules/copyPhotoDate.js ",
    "jpg_summary": "node modules/photSummaryGenerator.js ",
    "jpg": "run-s jpg_copy jpg_summary",
    "mkjson": "run-s md jpg_summary",
    "generate": "nuxt generate",
    "deploy": "run-s md jpg_summary lintfix generate",
    "pub": "run-s md jpg_summary lintfix generate build"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.14.0",
    "@nuxtjs/axios": "^5.12.0",
    "@nuxtjs/google-analytics": "^2.4.0",
    "@nuxtjs/markdownit": "^1.2.10",
    "@nuxtjs/pwa": "^3.0.0-beta.16",
    "@nuxtjs/sitemap": "^1.2.0",
    "@nuxtjs/style-resources": "^0.1.2",
    "fs": "0.0.1-security",
    "glob": "^7.1.4",
    "googleapis": "^39.2.0",
    "highlight.js": "^9.15.8",
    "nuxt": "^2.13.3",
    "sharp": "^0.25.4",
    "text-to-svg": "^3.1.5",
    "vue-image-lightbox": "^7.1.3",
    "vue-lazyload": "^1.3.3",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^4.12.0",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "processmd": "^4.4.2",
    "sass-loader": "^7.1.0"
  }
}
